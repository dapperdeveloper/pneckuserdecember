<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.callpneck"
    android:targetSandboxVersion="1">

    <uses-permission android:name="com.google.android.maps" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".PneckUserApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.registrationSecond.Activity.FullScreenImageActivity"></activity>
        <activity android:name=".activity.GoogleMapTrackingAddressSet" />
        <activity android:name=".activity.deliveryboy.DeliveryMainActivity" />
        <activity android:name=".activity.registrationSecond.Activity.OrderPlacedActivity" />
        <activity android:name=".activity.registrationSecond.Activity.MapsActivity" />
        <activity android:name=".activity.registrationSecond.Activity.ProviderDetailActivity" />
        <activity android:name=".activity.registrationSecond.Activity.OrderListActivity" />
        <activity android:name=".activity.registrationSecond.Activity.CheckoutActivity" />
        <activity
            android:name=".activity.registrationSecond.Activity.SearchLocationActivity"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity android:name=".Language.LanguageSettingActivity" />
        <activity android:name=".activity.registrationSecond.Activity.ShortByCategoryRestuarantActivity" />
        <activity android:name=".activity.registrationSecond.Activity.WorkMapActivity" />
        <activity android:name=".activity.registrationSecond.Activity.HomeMapActivity" />
        <activity android:name=".activity.registrationSecond.Activity.EditCartActivity" />
        <activity android:name=".activity.registrationSecond.Activity.AddToCartActivity" />
        <activity android:name=".activity.registrationSecond.Activity.ShopDetailActivity" />
        <activity
            android:name=".activity.registrationSecond.Activity.ShopHomeActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".activity.registrationSecond.Activity.ServiceDetailDescriptionActivity" />
        <activity android:name=".activity.registrationSecond.Activity.ServiceDetailActivity" />
        <activity android:name=".activity.registrationSecond.Activity.ProviderActivity" />
        <activity android:name=".activity.registrationSecond.Activity.SubCategoryActivity" />
        <activity android:name=".activity.registrationSecond.Activity.MyWalletActivity" />
        <activity android:name=".activity.registrationSecond.Activity.MyBookingActivity" />
        <activity android:name=".activity.registrationSecond.Activity.EmergencyContactActivity" />
        <activity android:name=".activity.registrationSecond.Activity.FavouriteProviderActivity" />
        <activity android:name=".activity.registrationSecond.Activity.NotificationsActivity" />
        <activity
            android:name=".activity.registrationSecond.Activity.SearchActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".activity.registrationSecond.Activity.InviteFriendActivity" />
        <activity android:name=".activity.registrationSecond.Activity.TransactionsActivity" />
        <activity android:name=".activity.registrationSecond.Activity.TransferMoneyActivity" />
        <activity android:name=".activity.registrationSecond.Activity.AddMoneyActivity" />
        <activity android:name=".taxi.activity.DriverListActivity" />
        <activity android:name=".taxi.activity.RealTimeActivity" />
        <activity android:name=".taxi.activity.DestinationPickerActivity" />
        <activity android:name=".taxi.TaxiMainActivity" />
        <activity
            android:name=".activity.registrationSecond.MainScreenActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.Vendor.VendorSearchActivity"
            android:label="@string/title_activity_vendor_search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.registrationSecond.LoginWithPhoneActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.SplashActivity" />
        <activity android:name=".activity.Vendor.DisplayCompleteVendor" />
        <activity android:name=".adapter.VendorAdapters" />
        <activity android:name=".activity.Vendor.VendorMainPage" />
        <activity
            android:name=".activity.SideMenuScreens.UserFeedbackScreen"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name=".activity.SideMenuScreens.WebPageScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SideMenuScreens.ReferAndEarn"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Registration.ChangePassOnForgetPassword"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SideMenuScreens.ChangePasswordScreen"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name=".activity.SideMenuScreens.WalletScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SideMenuScreens.EditProfileScreen"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name=".activity.SideMenuScreens.HelpScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SideMenuScreens.AboutUsScreen"
            android:screenOrientation="portrait" />
